
<%= form_for @user do |f| %>
	<%= f.label :name, "Enter Name" %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :email, "Enter Email" %>
	<%= f.text_field :email %>
	<br>
	<%= f.label :password, "Enter Password" %>
	<%= f.password_field :password %>
	<br>
	<br>

	<%= f.submit, "Create User" %>
<% end %>